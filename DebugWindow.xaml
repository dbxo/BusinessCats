        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BusinessCats"
        mc:Ignorable="d"
        Title="DebugWindow" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="238" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="292" Margin="0,31,0,0"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="0,1,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
